pipeline {
  agent none
  stages {
    stage('install-python') {
      agent {
        docker { 
          image 'python:3.9' // Use the Python Docker image
        }
      }
      steps {
        sh 'python3 --version'
      }
    }
    stage('install-flask') {
      agent {
        docker { 
          image 'python:3.9' // Use the Python Docker image
        }
      }
      steps {
        sh 'ps -ef'
      }
    }
  }
}
